<!DOCTYPE html>
<html lang="en">
<head>
</head>
<body>
    <div>
        <h1>Users</h1>
          <table class="table">
            <thead>
              <tr>
                <th>ID</th>
                <th>NAME</th>
                <th>EMAIL</th>
                <th>IS ADMIN</th>
                <th>ACTIONS</th>
              </tr>
            </thead>
            <tbody>
              <% users.forEach((user) => { %>
                <tr>
                  <td><%= user._id %></td>
                  <td><%= user.name %></td>
                  <td><%= user.email %></td>
                  <td><%= user.isAdmin ? 'YES' : 'NO' %></td>
                  <td></td>
                </tr>
              <% }); %>
            </tbody>
          </table>
       
      </div>
</body>
</html>