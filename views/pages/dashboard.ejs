<!DOCTYPE html>
<html lang="en">
  <head>
    <!-- Link Icon-->
    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200"
    />
    <!-- link css-->
    <link rel="stylesheet" href="/public/css/dashboard.css" />
  </head>

  <body>
    <div class="container">
      <main>
        <h1>Dashbored</h1>
        <div class="insights">
          <div class="sales">
            <span class="material-symbols-outlined">analytics</span>
            <div class="middle">
              <div class="left">
                <h3>Total Sales</h3>
                <h1>$25,024</h1>
              </div>
              <div class="progress">
                <!-- <svg>
                  <circle cx="38" cy="38" r="36"></circle>
                </svg> -->
                <div class="number">
                  <p>81%</p>
                </div>
              </div>
            </div>
            <small class="text-mutes">Last 24 Hours</small>
          </div>
          <!-- End of Sales -->
          <div class="expenses">
            <span class="material-symbols-outlined">bar_chart</span>
            <div class="middle">
              <div class="left">
                <h3>Total Expenses</h3>
                <h1>$14,160</h1>
              </div>
              <div class="progress">
                <div class="number">
                  <p>62%</p>
                </div>
              </div>
            </div>
            <small class="text-mutes">Last 24 Hours</small>
          </div>
          <!-- End of Expenses -->
          <div class="income">
            <span class="material-symbols-outlined">stacked_line_chart</span>
            <div class="middle">
              <div class="left">
                <h3>Total Income</h3>
                <h1>$10,864</h1>
              </div>
              <div class="progress">
                <div class="number">
                  <p>44%</p>
                </div>
              </div>
            </div>
            <small class="text-mutes">Last 24 Hours</small>
          </div>
          <!-- End of Income -->
        </div>
        <!-- End of insights -->
       
      </main>
      <!-- end of main-->
       
    </div>

  </body>
</html>
