<script>
    const addToCartHandler = async (productId) => {
      const fet = await fetch("/cart/add", {
        method: "POST",
        headers: {
          "Content-Type": "application/json",
        },
        body: JSON.stringify({ id: productId }),
      });
    
    if(fet.status === 200) {
        window.location.reload();
    }
    };
  </script>